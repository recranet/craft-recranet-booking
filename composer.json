{
  "name": "recranet/craft-recranet-booking",
  "description": "Synchronizes various Recranet Booking sources to Craft CMS",
  "type": "craft-plugin",
  "require": {
    "php": ">=8.2",
    "craftcms/cms": "^5.7.0"
  },
  "require-dev": {
    "craftcms/ecs": "dev-main",
    "craftcms/phpstan": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "recranet\\craftrecranetbooking\\": "src/"
    }
  },
  "extra": {
    "handle": "_recranet-booking",
    "name": "Recranet Booking",
    "developer": "Recranet",
    "documentationUrl": "",
    "class": "recranet\\craftrecranetbooking\\RecranetBooking"
  },
  "scripts": {
    "check-cs": "ecs check --ansi",
    "fix-cs": "ecs check --ansi --fix",
    "phpstan": "phpstan --memory-limit=1G"
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}